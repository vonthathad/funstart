{
  "name": "funstart-en",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "clean": "rm -rf dist && rm -rf src/app/*.ngfactory.ts && cd compiled && find . ! -name 'main-prod.ts' -type f -exec rm -f {} + && cd ..",
    "serve": "http-server . -p 5557",
    "ngc": "ngc -p src/tsconfig.json && cp src/app/* compiled",
    "build": "tsc -p tsconfig-tsc.json",
    "rollup": "rollup -f iife -c rollup.config.js -o dist/bundle.es2015.js",
    "es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run clean && npm run ngc && npm run build && npm run rollup && npm run es5 && npm run minify"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/flex-layout": "^2.0.0-beta.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "3.4.1",
    "@angular2-material/progress-circle": "^2.0.0-alpha.8-2",
    "angular2-infinite-scroll": "^0.2.6",
    "angular2-modal": "^2.0.2",
    "angulartics2": "^1.6.3",
    "core-js": "^2.4.1",
    "ng2-facebook-sdk": "^1.2.0",
    "ng2-translate": "^5.0.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.31",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/tsc-wrapped": "^0.5.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.21",
    "codelyzer": "2.0.0-beta.4",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "^4.0.0",
    "tslint-loader": "^3.2.1",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
